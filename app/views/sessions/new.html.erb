<h1>Login</h1>

<form action="<%=session_url%>" method="post">
  <label>User Email
  <input type="text" name="email">
  </label>
  <br>
  <label>Password
  <input type="password" name="password">
  </label>
  <br>

  <input type="submit" name="commit" value="Login">
</form>
<%= button_to "Sign Up", new_user_url , method: "get"%>